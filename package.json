{
  "name": "arbor",
  "version": "0.0.1",
  "description": "Seamless state management made with <3",
  "author": "drborges <drborges.cic@gmail.com> (https://github.com/drborges)",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "src/index.jsx",
  "jsnext:main": "src/index.jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/drborges/arbor.git"
  },
  "keywords": [
    "arbor",
    "redux",
    "state",
    "functional",
    "immutable",
    "replay",
    "flux"
  ],
  "bugs": {
    "url": "https://github.com/drborges/arbor/issues"
  },
  "homepage": "https://github.com/drborges/arbor",
  "scripts": {
    "build": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config webpack.config.js",
    "build:watch": "./node_modules/webpack/bin/webpack.js --config webpack.config.js --watch",
    "test": "mocha",
    "test:watch": "mocha -w",
    "flow:watch": "flow-watch",
    "prepare": "yarn build",
    "preversion": "yarn test",
    "version": "yarn build",
    "postversion": "git push && git push --tags",
    "release-patch": "npm version patch -m",
    "release-minor": "npm version minor -m",
    "release-major": "npm version major -m",
    "publish": "npm login && npm publish ./"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-computed-properties": "^6.22.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^1.0.0-beta.0",
    "compression-webpack-plugin": "^1.1.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "flow-bin": "^0.42.0",
    "flow-watch": "^1.1.1",
    "growl": "^1.9.2",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "webpack": "^2.3.2",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.4.2"
  }
}
